{
    "datacenter": "dc1.guagua.party",
    "data_dir": "/opt/consul",
    "log_level": "INFO",
    "node_name": "c1",
    "server": true,
    "bind_addr": "{{ GetPrivateIP }}",
    "bootstrap_expect": 2,
    "ui": true,
    "client_addr": "0.0.0.0",
    "enable_script_checks": false,
    "enable_local_script_checks": true,
    "enable_debug": false,
    "enable_syslog": true,
    "encrypt": "6jLwGWeMvJjLKnDAO8h2LQ==",
    "encrypt_verify_incoming": true,
    "encrypt_verify_outgoing": true,
    "retry_join": [
        "192.168.0.137",
        "192.168.0.185",
        "192.168.0.131"
    ],
    "addresses": {
        "dns": "{{ GetPrivateIP }}",
        "http": "{{ GetPrivateIP }}",
        "https": "{{ GetPrivateIP }}",
        "grpc": "{{ GetPrivateIP }}"
    },
    "ports": {
        "dns": 8600,
        "http": 8500,
        "https": 8501,
        "grpc": 8502,
        "serf_lan": 8301,
        "serf_wan": 8302,
        "server": 8300,
        "sidecar_min_port": 21000,
        "sidecar_max_port": 21255
    },
    "acl": {
        "enabled": true,
        "policy_ttl": "30s"
    },
    "advertise_addr": "{{ GetPrivateIP }}",
    "serf_wan": "{{ GetPrivateIP }}",
    "serf_lan": "{{ GetPrivateIP }}",
    "advertise_addr_wan": "{{ GetPrivateIP }}",
    "autopilot": {
        "cleanup_dead_servers": true,
        "last_contact_threshold": "200ms",
        "max_trailing_logs": 250,
        "server_stabilization_time": "10s"
    },
    "check_update_interval": "10m",
    "domain": "consul",
    "dns_config": {
        "allow_stale": true,
        "max_stale": "24h",
        "node_ttl": "0s",
        "service_ttl": {
            "*": "0s"
        },
        "enable_truncate": true,
        "only_passing": true,
        "recursor_timeout": "2s",
        "disable_compression": false,
        "enable_additional_node_meta_txt": true
    },
    "disable_keyring_file": false,
    "http_config": {
        "block_endpoints":[
            "/v1/acl",
            "/v2/acl"
        ],
        "response_headers": {
            "Access-Control-Allow-Origin": "*"
          }
    }
}